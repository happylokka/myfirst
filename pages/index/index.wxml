<!--index.wxml-->
  <swiper
    indicator-dots="{{indicatorDots}}"
    autoplay="{{autoplay}}"
    interval="{{interval}}"
    duration="{{duration}}"
  >
    <block wx:for="{{imgUrls}}" wx:key="key">
      <swiper-item>
        <image src="{{item}}" class="slide-image" width="355" height="150" />
      </swiper-item>
    </block>
  </swiper>
  <view class="list-item">
    <view class='pro-item' wx:for='{{proList}}' bindtap='toDetail' wx:key='index' data-index='{{index}}'>
      <image class='pro-logo' src='{{item.img}}'></image>
      <view class='pro-body'>
        <view class='pro-title'>{{item.title}}</view>
        <!-- <text class='pro-desc'>{{item.shortDesc}}</text> -->
        <view class='pro-footer'>
          <image class='btn-detail' src='../../images/btn_detail.png'></image>
          <button open-type="contact" class="btn-ask"><image src='../../images/btn_ask.png' /></button>
        </view>
      </view>
    </view>
  </view>
